<div id="<%= dom_id shipping_line %>" class="w-full sm:w-auto my-5 space-y-5">
  <div>
    <strong class="block font-medium mb-1">Name:</strong>
    <%= shipping_line.name %>
  </div>
  <div>
    <strong class="block font-medium mb-1">ISO Code:</strong>
    <span class="inline-flex items-center px-2.5 py-0.5 rounded-full text-xs font-medium bg-blue-100 text-blue-800">
      <%= shipping_line.iso_code %>
    </span>
  </div>
</div>
